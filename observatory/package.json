{
  "name": "canton-ecosystem-observatory",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "Neutral, factual reference for the Canton Network ecosystem",
  "scripts": {
    "build": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/integration/",
    "validate": "tsx scripts/validate-data.ts",
    "collect": "tsx scripts/collect-github.ts",
    "migrate": "tsx scripts/migrate.ts",
    "generate": "tsx scripts/generate-site.ts"
  },
  "license": "BSD-0-Clause",
  "devDependencies": {
    "typescript": "5.7.3",
    "vitest": "3.0.5",
    "@vitest/coverage-v8": "3.0.5",
    "tsx": "4.19.2",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1"
  }
}
